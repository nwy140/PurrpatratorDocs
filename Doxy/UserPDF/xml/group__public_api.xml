        <definition>void Photon.Pun.IPunObservable.OnPhotonSerializeView</definition>
        <argsstring>(PhotonStream stream, PhotonMessageInfo info)</argsstring>
        <name>OnPhotonSerializeView</name>
        <reimplementedby refid="class_timer_script_1a6d1ceaff807c5794ce7540bab3020049">OnPhotonSerializeView</reimplementedby>
